#ranking
  #user-stuff
    = render 'user_info'
  = render 'info_modal'


  #player-list
    - @players.each do |player|
      .player-entry
        .place
          = "#{player.place_in_ranking}."
        .nickname{class: "#{nickname_class(player)}"}
          = "#{player.nickname}"
        .score
          = player.best_score



